import java.util.Scanner;
public class Main {

     public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a,b,c,d;
        a=sc.nextInt();
         for (int i=0; i<a; i++) {
             b=sc.nextInt();
             c=sc.nextInt();
             d=(b/3)*(c/3);
             System.out.println(d);
         }
    }
